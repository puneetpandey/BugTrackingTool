<%= form_for([@project, @project_membership]) do | f | %>
  <div class="form-group">
    <%= f.select( :user_id, options_from_collection_for_select(User.all, :id, :username), {}, { class: "form-control" } ) %>
  </div>

  <div class="form-group">
    <%= f.submit "Save", class: "btn btn-primary btn-lg btn-block" %>
  </div>
<% end %>